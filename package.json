    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "ink-testing-library": "^2.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.4.1",
    "madge": "^4.0.1",
    "mem-fs": "^2.2.1",
    "mem-fs-editor": "^9.4.0",
    "oclif": "^2.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.0",
    "semantic-release": "^19.0.2",
    "serialize-error": "^5.0.0",
    "ts-jest": "^27.1.3",
    "tsc-watch": "^4.6.0",
    "tscpaths": "^0.0.9",
    "typescript": ">= 4.4.4 < 4.7.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "resolutions": {
    "lodash": ">= 4.5.0",
    "lodash.template": ">= 4.5.0",
    "sql": ">= 0.78.0"
  }
}
