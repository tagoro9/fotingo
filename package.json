{
  "name": "fotingo",
  "version": "1.9.2",
  "description": "A CLI to ease the interaction between git, github and jira.",
  "main": "lib/fotingo.js",
  "bin": {
    "fotingo": "./lib/fotingo.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "build": "npm run lint && npm run clean && gulp build && chmod 755 ./lib/fotingo.js",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "release": "npm run build && standard-version",
    "test": "jest",
    "watch": "gulp watch"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Victor Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tagoro9/fotingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tagoro9/fotingo.git"
  },
  "keywords": [
    "jira",
    "github",
    "git"
  ],
  "dependencies": {
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "conventional-commits-parser": "^2.0.0",
    "debug": "^3.1.0",
    "github": "^11.0.0",
    "node-emoji": "^1.8.1",
    "nodegit": "^0.20.2",
    "ramda": "^0.25.0",
    "read": "^1.0.7",
    "readline": "^1.3.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/jest": "21.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^7.0.0",
    "gulp-newer": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0"
  },
  "engines": {
    "node": ">=6.3.1",
    "yarn": ">=0.19.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "coverageReporters": [
      "html"
    ]
  }
}
