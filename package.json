{
  "name": "fotingo",
  "version": "1.6.1",
  "description": "A CLI to ease the interaction between git, github and jira.",
  "main": "lib/fotingo.js",
  "bin": {
    "fotingo": "./lib/fotingo.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run lint && npm run clean && gulp build && chmod 755 ./lib/fotingo.js",
    "format": "prettier \"src/**/*.js\" --print-width 100 --single-quote --trailing-comma all --write",
    "lint": "eslint src test",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "release": "npm run build && standard-version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "gulp watch"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --print-width 100 --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "author": "Victor Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tagoro9/fotingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tagoro9/fotingo.git"
  },
  "keywords": [
    "jira",
    "github",
    "git"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "conventional-commits-parser": "^1.3.0",
    "debug": "^2.2.0",
    "github": "^9.1.0",
    "node-emoji": "^1.4.1",
    "nodegit": "^0.18.0",
    "ramda": "^0.21.0",
    "read": "^1.0.7",
    "readline": "^1.3.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.18.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.0.0",
    "gulp-newer": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.0.2",
    "rimraf": "^2.5.4",
    "standard-version": "^4.0.0"
  },
  "engines": {
    "node": "^6.3.1",
    "yarn": ">=0.19.1"
  }
}
