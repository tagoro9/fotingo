{
  "name": "fotingo",
  "version": "3.0.0",
  "description": "A CLI to ease the interaction between git, github and jira.",
  "main": "lib/index.js",
  "bin": {
    "fotingo": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p ./ && yarn build:fix-paths",
    "build:fix-paths": "tscpaths -p ./tsconfig.json -s ./src -o ./lib",
    "circular-dependencies": "madge --circular src",
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "lint": "eslint ./src ./test ./types --color --ext .ts --ext .tsx",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn clean && yarn build && oclif-dev manifest && oclif-dev readme",
    "prepare": "yarn build",
    "test": "FORCE_COLOR=0 jest",
    "version": "oclif-dev readme && git add README.md",
    "watch": "tsc-watch -p ./ --onSuccess 'yarn build:fix-paths'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fotingo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "author": "Victor Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tagoro9/fotingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tagoro9/fotingo.git"
  },
  "keywords": [
    "jira",
    "github",
    "git"
  ],
  "dependencies": {
    "@keyv/sqlite": "^2.0.1",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^3.1.0",
    "@octokit/rest": "^18.0.3",
    "@scarf/scarf": "^1.0.6",
    "autobind-decorator": "^2.4.0",
    "chalk": "^4.0.0",
    "conventional-commits-parser": "^3.0.1",
    "cosmiconfig": "^7.0.0",
    "debug": "^4.1.1",
    "editor": "^1.0.0",
    "escape-html": "^1.0.3",
    "escape-string-regexp": "^4.0.0",
    "figures": "^3.2.0",
    "fuse.js": "^6.0.4",
    "git-url-parse": "^11.1.2",
    "humanize-duration": "^3.18.0",
    "ink": "^2.3.0",
    "ink-spinner": "^3.1.0",
    "ink-text-input": "^3.2.0",
    "keyv": "^4.0.0",
    "node-emoji": "^1.10.0",
    "p-limit": "^3.1.0",
    "ramda": "^0.27.1",
    "react": "^16.8.6",
    "read": "^1.0.7",
    "readline": "^1.3.0",
    "request": "^2.88.0",
    "rxjs": "^6.5.1",
    "simple-git": "^2.1.0",
    "sqlite3": "^4.0.8",
    "tmp": "^0.2.0",
    "turndown": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@octokit/types": "^6.3.0",
    "@jest/globals": "^26.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/debug": "^4.1.4",
    "@types/escape-html": "^1.0.0",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/faker": "^5.1.5",
    "@types/figures": "^3.0.1",
    "@types/git-url-parse": "^9.0.0",
    "@types/humanize-duration": "^3.18.0",
    "@types/ink-spinner": "^3.0.0",
    "@types/jest": "^26.0.0",
    "@types/keyv": "^3.1.0",
    "@types/node": "^14.0.13",
    "@types/node-emoji": "^1.8.1",
    "@types/ramda": "^0.27.6",
    "@types/react": "^17.0.0",
    "@types/request": "^2.48.1",
    "@types/serialize-error": "^4.0.1",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-typescript-sort-keys": "^1.2.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "faker": "^4.1.0",
    "husky": "^4.0.0",
    "ink-testing-library": "^2.0.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.0.7",
    "madge": "^3.4.4",
    "oclif": "^1.15.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.0.7",
    "serialize-error": "^7.0.0",
    "ts-jest": "^26.0.0",
    "tsc-watch": "^4.2.9",
    "tscpaths": "^0.0.9",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "resolutions": {
    "lodash": ">= 4.5.0",
    "lodash.template": ">= 4.5.0",
    "sql": ">= 0.78.0"
  }
}
