{
  "name": "fotingo",
  "version": "4.0.0",
  "description": "A CLI to ease the interaction between git, github and jira.",
  "main": "lib/index.js",
  "bin": {
    "fotingo": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p ./ && yarn build:fix-paths",
    "build:fix-paths": "tscpaths -p ./tsconfig.json -s ./src -o ./lib",
    "circular-dependencies": "madge --circular src",
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "lint": "eslint ./src ./test ./types --color --ext .ts --ext .tsx",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn clean && yarn build && oclif-dev manifest && oclif-dev readme",
    "prepare": "husky install",
    "test": "jest --coverage",
    "version": "oclif-dev readme && git add README.md",
    "watch": "tsc-watch -p ./ --onSuccess 'yarn build:fix-paths'"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fotingo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "author": "Victor Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tagoro9/fotingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tagoro9/fotingo.git"
  },
  "keywords": [
    "jira",
    "github",
    "git"
  ],
  "dependencies": {
    "@keyv/sqlite": "^3.6.3",
    "@oclif/command": "^1.8.20",
    "@oclif/config": "^1.18.6",
    "@oclif/core": "^1.20.4",
    "@oclif/plugin-help": "^5.1.19",
    "@octokit/rest": "^19.0.5",
    "@scarf/scarf": "^1.0.6",
    "@types/env-ci": "^3.1.1",
    "autobind-decorator": "^2.4.0",
    "chalk": "^4.0.0",
    "conventional-commits-parser": "^3.2.4",
    "cosmiconfig": "^8.0.0",
    "debug": "^4.3.4",
    "editor": "^1.0.0",
    "env-ci": "^7.3.0",
    "escape-string-regexp": "^4.0.0",
    "figures": "^3.2.0",
    "fuse.js": "^6.6.2",
    "git-url-parse": "^13.1.0",
    "humanize-duration": "^3.27.3",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^4.0.3",
    "keyv": "^4.5.2",
    "node-emoji": "^1.11.0",
    "open": "^8.4.0",
    "p-all": "^3.0.0",
    "ramda": "^0.27.1",
    "react": "^17.0.0",
    "read": "^1.0.7",
    "readline": "^1.3.0",
    "request": "^2.88.0",
    "rxjs": "^7.5.7",
    "sanitize-html": "^2.7.3",
    "simple-git": "^3.15.1",
    "sqlite3": "^5.1.2",
    "tmp": "^0.2.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@faker-js/faker": "^7.6.0",
    "@jest/globals": "^29.3.1",
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.2.12",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.7",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/figures": "^3.0.1",
    "@types/git-url-parse": "^9.0.1",
    "@types/humanize-duration": "^3.18.0",
    "@types/ink-spinner": "^3.0.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.10",
    "@types/node-emoji": "^1.8.2",
    "@types/ramda": "^0.27.39",
    "@types/react": "^17.0.0",
    "@types/request": "^2.48.8",
    "@types/sanitize-html": "^2.6.2",
    "@types/serialize-error": "^4.0.1",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
