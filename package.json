    "eslint-plugin-unicorn": "^45.0.1",
    "husky": "^8.0.2",
    "ink-testing-library": "^2.1.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
    "madge": "^5.0.1",
    "mem-fs": "^2.2.1",
    "mem-fs-editor": "^9.5.0",
    "oclif": "^3.3.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^19.0.2",
    "serialize-error": "^5.0.0",
    "ts-jest": "^29.0.3",
    "tsc-watch": "^5.0.3",
    "tscpaths": "^0.0.9",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "resolutions": {
    "lodash": ">= 4.5.0",
    "lodash.template": ">= 4.5.0",
    "sql": ">= 0.78.0"
  }
}
