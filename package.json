{
  "name": "fotingo",
  "version": "2.0.0-beta.8",
  "description": "A CLI to ease the interaction between git, github and jira.",
  "main": "lib/fotingo.js",
  "bin": {
    "fotingo": "./lib/fotingo.js"
  },
  "scripts": {
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "build": "yarn run clean && tsc -p ./ && chmod 755 ./lib/fotingo.js",
    "lint": "tslint -p .",
    "prepare": "yarn run build",
    "release": "yarn run build && standard-version",
    "watch": "tsc -w -p ./",
    "test": "jest",
    "circular-dependencies": "madge --circular src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Victor Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tagoro9/fotingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tagoro9/fotingo.git"
  },
  "keywords": [
    "jira",
    "github",
    "git"
  ],
  "dependencies": {
    "@keyv/sqlite": "^2.0.1",
    "@octokit/rest": "^16.25.1",
    "autobind-decorator": "^2.4.0",
    "chalk": "^3.0.0",
    "conventional-commits-parser": "^3.0.1",
    "cosmiconfig": "^6.0.0",
    "debug": "^4.1.1",
    "editor": "^1.0.0",
    "escape-string-regexp": "^2.0.0",
    "fuse.js": "^3.4.5",
    "git-url-parse": "^11.1.2",
    "humanize-duration": "^3.18.0",
    "ink": "^2.3.0",
    "ink-select-input": "^3.1.1",
    "ink-spinner": "^3.0.1",
    "ink-text-input": "^3.2.0",
    "keyv": "^3.1.0",
    "module-alias": "^2.2.0",
    "node-emoji": "^1.10.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "read": "^1.0.7",
    "readline": "^1.3.0",
    "request": "^2.88.0",
    "rxjs": "^6.5.1",
    "simple-git": "^1.113.0",
    "sqlite3": "^4.0.8",
    "tmp": "^0.1.0",
    "turndown": "^5.0.3",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/faker": "^4.1.5",
    "@types/git-url-parse": "^9.0.0",
    "@types/humanize-duration": "^3.18.0",
    "@types/jest": "^24.0.12",
    "@types/keyv": "^3.1.0",
    "@types/node": "^12.12.20",
    "@types/node-emoji": "^1.8.1",
    "@types/ramda": "^0.26.33",
    "@types/request": "^2.48.1",
    "@types/serialize-error": "^4.0.1",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.0",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "madge": "^3.4.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "serialize-error": "^5.0.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=6.3.1",
    "yarn": ">=0.19.1"
  },
  "_moduleAliases": {
    "src": "./lib"
  },
  "resolutions": {
    "lodash": ">= 4.5.0",
    "lodash.template": ">= 4.5.0",
    "sql": ">= 0.78.0"
  }
}
