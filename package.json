{
  "name": "fotingo",
  "version": "3.6.2",
  "description": "A CLI to ease the interaction between git, github and jira.",
  "main": "lib/index.js",
  "bin": {
    "fotingo": "./bin/run"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "yarn clean && tsc -p ./ && yarn build:fix-paths",
    "build:fix-paths": "tscpaths -p ./tsconfig.json -s ./src -o ./lib",
    "circular-dependencies": "madge --circular src",
    "clean": "rimraf lib",
    "coverage": "jest --coverage --silent",
    "lint": "eslint ./src ./test ./types --color --ext .ts --ext .tsx",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn clean && yarn build && oclif-dev manifest && oclif-dev readme",
    "prepare": "husky install",
    "test": "FORCE_COLOR=0 jest",
    "version": "oclif-dev readme && git add README.md",
    "watch": "tsc-watch -p ./ --onSuccess 'yarn build:fix-paths'"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ]
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fotingo",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "author": "Victor Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tagoro9/fotingo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tagoro9/fotingo.git"
  },
  "keywords": [
    "jira",
    "github",
    "git"
  ],
  "dependencies": {
    "@keyv/sqlite": "^3.0.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.2",
    "@oclif/plugin-help": "^5.1.10",
    "@octokit/rest": "^18.10.0",
    "@scarf/scarf": "^1.0.6",
    "@types/env-ci": "^3.1.0",
    "autobind-decorator": "^2.4.0",
    "chalk": "^4.0.0",
    "conventional-commits-parser": "^3.2.4",
    "cosmiconfig": "^7.0.1",
    "debug": "^4.3.3",
    "editor": "^1.0.0",
    "env-ci": "^5.0.2",
    "escape-string-regexp": "^4.0.0",
    "figures": "^3.2.0",
    "fuse.js": "^6.5.3",
    "git-url-parse": "^11.6.0",
    "humanize-duration": "^3.18.0",
    "ink": "^2.3.0",
    "ink-spinner": "^3.1.0",
    "ink-text-input": "^3.2.0",
    "keyv": "^4.0.5",
    "node-emoji": "^1.11.0",
    "p-all": "^3.0.0",
    "ramda": "^0.27.1",
    "react": "^16.8.6",
    "read": "^1.0.7",
    "readline": "^1.3.0",
    "request": "^2.88.0",
    "rxjs": "^7.5.5",
    "sanitize-html": "^2.7.0",
    "simple-git": "^3.1.0",
    "sqlite3": "^5.0.3",
    "tmp": "^0.2.0",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@jest/globals": "^27.4.6",
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.4",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/faker": "^5.5.8",
    "@types/figures": "^3.0.1",
    "@types/git-url-parse": "^9.0.0",
    "@types/humanize-duration": "^3.18.0",
    "@types/ink-spinner": "^3.0.0",
    "@types/jest": "^27.4.0",
    "@types/keyv": "^3.1.0",
    "@types/node": "^14.0.13",
    "@types/node-emoji": "^1.8.1",
    "@types/ramda": "^0.27.39",
    "@types/react": "^17.0.0",
    "@types/request": "^2.48.1",
    "@types/sanitize-html": "^2.6.2",
    "@types/serialize-error": "^4.0.1",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
