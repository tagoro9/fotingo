{
  "extends": [
    "prettier",
    "airbnb"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "space-before-function-paren": 0,
    "indent": 0
  },
  "plugins": [],
  "parser": "babel-eslint",
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": [
        "node_modules",
        "src"
      ]
    }
  },
  "globals": {}
}
